{
  "name": "ng-action-outlet-srcs",
  "version": "0.0.0-development",
  "homepage": "https://klemenoslaj.github.io/ng-action-outlet/",
  "repository": {
    "type": "git",
    "url": "https://github.com/klemenoslaj/ng-action-outlet.git"
  },
  "scripts": {
    "build:core": "ng build core",
    "docs:coverage": "compodoc ./projects/core -p ./projects/core/tsconfig.docs.json --disableLifeCycleHooks --disableInternal --coverageMinimumPerFile 100",
    "docs:build.prod": "yarn docs:coverage && compodoc ./projects/core -p ./projects/core/tsconfig.docs.json --disableLifeCycleHooks --disableInternal --disablePrivate --disableProtected --name NgActionOutlet --output docs",
    "docs:build.serve": "compodoc ./projects/core -p ./projects/core/tsconfig.docs.json --disableLifeCycleHooks --disableInternal -s -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.4.0",
    "travis-deploy-once": "^5.0.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.3",
    "@angular-devkit/build-ng-packagr": "^0.13.3",
    "@angular/cli": "^7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@compodoc/compodoc": "^1.1.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.0",
    "semantic-release": "^15.10.5",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "^5.13.0",
    "typescript": "~3.2.0"
  },
  "release": {
    "pkgRoot": "dist/core"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">= 8.12.0",
    "yarn": ">= 1.10.1"
  }
}
